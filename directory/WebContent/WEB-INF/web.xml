<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>directory</display-name><servlet>
    <servlet-name>directory</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>directory</servlet-name>
    <url-pattern>/</url-pattern>
    <url-pattern>/login</url-pattern>
    <url-pattern>/registration</url-pattern>
    <url-pattern>/personList</url-pattern>
    <url-pattern>/personForm</url-pattern>
    <url-pattern>/editPassword</url-pattern>
    <url-pattern>/sendPassword</url-pattern>
    <url-pattern>/admin</url-pattern>
    <url-pattern>/admin/login</url-pattern>
    <url-pattern>/admin/adminManager</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>403</error-code>
    <location>/E403</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/E404</location>
  </error-page>
  <error-page>
  	<error-code>500</error-code>
  	<location>/E500</location>
  </error-page>
</web-app>